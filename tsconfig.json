{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"outDir": "./dist",
		"target": "ESNext",
		"moduleResolution": "node",
		"baseUrl": ".",
		"paths": {
			"@app/*": ["src/*"],
			"@assets/*": ["assets/*"],
			"@animations/*": ["assets/animations/*"],
			"@components/*": ["src/components/*"],
			"@contexts/*": ["src/contexts/*"],
			"@services/*": ["src/services/*"],
			"@views/*": ["src/views/*"],
			"@utils/*": ["src/utils/*"]
		}
	}
}
